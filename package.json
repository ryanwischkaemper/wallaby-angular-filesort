{
  "name": "wallaby-angular-filesort",
  "version": "0.0.1",
  "description": "WallabyJS preprocessor to sort AngularJS files",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "test": "mocha --require babel-core/register --compilers js:babel-core/register test/*.spec.js",
    "tdd": "mocha --watch --require babel-core/register --compilers js:babel-core/register test/*.spec.js"
  },
  "keywords": [
    "angular",
    "filesort",
    "wallaby",
    "preprocessor"
  ],
  "author": "Ryan Wischkaemper",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.11.1",
    "minimatch": "^3.0.0",
    "ng-dependencies": "^0.5.0",
    "toposort": "^0.2.12"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  }
}
